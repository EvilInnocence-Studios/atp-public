    "@eslint/js": "^9.15.0",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/superagent": "^8.1.9",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "chalk": "^5.3.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "sass": "^1.83.0",
    "table": "^6.9.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
