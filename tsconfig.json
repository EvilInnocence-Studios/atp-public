{
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@public/*": ["src/public/*"],
      "@core/*": ["src/core/*"],
      "@common/*": ["src/common/*"],
      "@uac/*": ["src/uac/*"],
      "@uac-shared/*": ["src/uac-shared/*"],
      "@core-shared/*": ["src/core-shared/*"],
      "@common-shared/*": ["src/common-shared/*"],
      "@store/*": ["src/store/*"],
      "@store-shared/*": ["src/store-shared/*"],
      "@styles/*": ["styles/*"],
      "@config": ["src/config"]
    }
  },
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ]
}
